<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<Authors>Peter Waher</Authors>
		<Company>Waher Data AB</Company>
		<Product>Waher.IoTGateway</Product>
		<Description>Class library that helps applications compute statistical aggregates from sampled values.  dataStatistical computations can be performed in-memory or using persisted buckets.</Description>
		<Copyright>Copyright © Waher Data AB 2025-2026. All rights reserved.</Copyright>
		<PackageLicenseUrl />
		<PackageProjectUrl>https://github.com/PeterWaher/IoTGateway/tree/master/Runtime/Waher.Runtime.Settings</PackageProjectUrl>
		<RepositoryUrl>https://github.com/PeterWaher/IoTGateway</RepositoryUrl>
		<RepositoryType>GitHub</RepositoryType>
		<PackageTags>persistence settings</PackageTags>
		<NeutralLanguage>English</NeutralLanguage>
		<AssemblyVersion>1.0.0</AssemblyVersion>
		<FileVersion>1.0.0</FileVersion>
		<Version>1.0.0</Version>
		<PackageIconUrl />
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageIcon>Icon_64x64.png</PackageIcon>
	</PropertyGroup>
	<PropertyGroup>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\..\Images\Icon_64x64.png">
			<Pack>True</Pack>
			<PackagePath />
		</None>
		<None Include="..\..\LICENSE.txt">
			<Pack>True</Pack>
			<PackagePath />
		</None>
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\..\Content\Waher.Content\Waher.Content.csproj" />
	  <ProjectReference Include="..\..\Persistence\Waher.Persistence\Waher.Persistence.csproj" />
	  <ProjectReference Include="..\..\Script\Waher.Script\Waher.Script.csproj" />
	  <ProjectReference Include="..\Waher.Runtime.Collections\Waher.Runtime.Collections.csproj" />
	</ItemGroup>

</Project>
