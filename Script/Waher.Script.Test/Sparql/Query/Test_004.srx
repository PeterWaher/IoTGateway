<?xml version="1.0"?>
<sparql xmlns="http://www.w3.org/2005/sparql-results#"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.w3.org/2001/sw/DataAccess/rf1/result2.xsd">

	<head>
		<variable name="x"/>
		<variable name="name"/>
		<variable name="quoted"/>

		<link href="example-quoted.rq" />
	</head>

	<results>

		<result>
			<binding name="x">
				<bnode>r1</bnode>
			</binding>
			<binding name="name">
				<literal>Alice</literal>
			</binding>
			<binding name="quoted">
				<triple>
					<subject>
						<uri>http://example.org/alice</uri>
					</subject>
					<predicate>
						<uri>http://example.org/name</uri>
					</predicate>
					<object>
						<literal datatype="http://www.w3.org/2001/XMLSchema#string">Alice</literal>
					</object>
				</triple>
			</binding>
		</result>

		<result>
			<binding name="x">
				<bnode>r2</bnode>
			</binding>
			<binding name="name">
				<literal xml:lang="en">Bob</literal>
			</binding>
			<binding name="quoted">
				<triple>
					<subject>
						<uri>http://example.org/bob</uri>
					</subject>
					<predicate>
						<uri>http://example.org/name</uri>
					</predicate>
					<object>
						<literal datatype="http://www.w3.org/2001/XMLSchema#string">Bob</literal>
					</object>
				</triple>
			</binding>
		</result>

	</results>

</sparql>
