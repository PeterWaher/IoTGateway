/* #endregion dropdown*/

/* #region links */

a
{
    word-wrap: break-word;
    color: ¤LinkColorUnvisited¤;
}

a:hover
{
    color: ¤LinkColorHot¤;
}

section a:link
{
    color: ¤LinkColorUnvisited¤;
}

section a:visited
{
    color: ¤LinkColorVisited¤;
}

section a:hover
{
    color: ¤LinkColorHot¤;
}

section a:active
{
    color: ¤LinkColorHot¤;
}

/* #endregion links */

/* #region Carousel*/

.native-carousel
{
    overflow: hidden;
    position: relative;
    display: grid;
    justify-content: center;
    align-items: center;
    width: 100%;
}

.native-carousel > * {
    grid-area: 1 / 1;
    margin-inline: auto;
    pointer-events: none;
    transition: opacity 0.3s, transform 0.2s;
    opacity: 0;
}

.native-carousel > *[data-carousel-right]
{
    transform: translateX(100%);
    transition: opacity 0.1s, transform 0.2s;
}
.native-carousel > *[data-carousel-left]
{
    transform: translateX(-100%);
    transition: opacity 0.1s, transform 0.2s;
}

.native-carousel > *[data-carousel-active]
{
    opacity: 1;
    pointer-events: initial;
}

/* #endregion Carousel*/


/* #region Backdrop */
#native-backdrop
{
    position: fixed;
    top: 0;
    left: 0;
    height: 100vh;
    width: 100vw;
    background-color: black;
    opacity: 0.35;
    z-index: 1000; 
}
/* #region Backdrop */


/* #region Popups */

#native-popup-container:empty
{
    display: none;
}

#native-popup-container
{
    overflow: hidden;
    position: fixed;
    background: ¤Alpha(BackgroundColor, 255)¤;
    min-width: min(20rem, 90%);
    max-width: 90%;
    padding: 1.5rem;
    border-radius: 1rem;
    border: 1px ¤Blend(BackgroundColor, "black", 0.7)¤ solid;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 1001;
}

#native-popup-container > *
{
    margin: 0;
    padding: 0;
}

/* #endregion Popups */

