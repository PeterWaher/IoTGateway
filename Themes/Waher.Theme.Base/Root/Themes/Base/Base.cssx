@media screen and (min-width: 901px) {
    /* #region Header */

    #native-header 
    {
        padding-block: 1rem;
    }

    #native-header > nav > ul > li > p
    #native-header > nav > ul > li > a
    {
        height: 100%;
    }

    #native-header nav > ul > li > ul[expanded]
    {
        display:block;
        position: fixed;
        width: max-content;
        margin: 0;
        padding: 0;
    }

    #native-header nav ul > li > p
    #native-header nav ul > li > a
    {
        text-align: left;
    }

    #native-header nav li > ul > li:hover
    {
        background-color: transparent;
    }

    #native-header nav li > ul > li
    {
        display: flex;
        flex-direction: row-reverse;
        background-color: transparent;
        overflow: visible;
        border-bottom: none;
        width: fit-content;
        margin-right: auto;
    }


    /* #endregion Header */


    /*#region Dropdown*/

    .native-dropdown-button
    {
        display: none;
    }

    #native-header li:hover > p
    #native-header li:hover > a 
    {
        color: inherit !important;
    }

    #native-header li > ul > li > p
    #native-header li > ul > li > a 
    {
        height: 100%;
    }

    #native-header nav > ul > li > ul
    {
        border-top: 1px transparent solid;
    }

    #native-header li > ul
    {
        margin-inline-start: 0;
        margin-left: 0;
        overflow-y: auto;
        direction:rtl; 
    }

    header#native-header li > ul > li:not(:last-of-type) > p
    header#native-header li > ul > li:not(:last-of-type) > a
    {
        border-bottom: none;
    }
    
    
    #native-header li > ul > li > ul[expanded]
    {
        display: block;
    }

    #native-header li > ul > li > ul
    {
        padding: 0px;
        display: none;
    }

    #native-header > nav a,
    #native-header > nav p,
    {
        text-align: center;
    }


    /*#endregion Dropdown*/
}

@media screen and (max-width: 900px) {
    main
    {
        width: 100%;
    }

    /* #region TOC */

    .toc
    {
        padding: 0;
        max-width: 100%;
    }

    div.tocBody ol
    {
        padding-left: 0.5rem;
    }

    /* #region TOC */

    /* #region Header */

    body #native-header {
        max-height: 100%;
    }
    
    body header#native-header {
        background-position: initial;
        background-repeat: initial;
        background-size: initial;
        padding-top: 0;
        padding-bottom: 1rem;
    }

    #native-header > nav {
        display: flex;
        flex-direction: column;
        max-height: 100vh;
        padding-top: 1rem;
    }
    
    #native-header > nav > ul {
        flex-shrink: 1;
    }
    
    #native-header[data-visible] > nav > div {
        margin-bottom: 1rem;
    }
    
    #native-header > nav > ul {
        margin-bottom: 1rem;
        overflow-y: auto;
    }

    header#native-header li > *
    {
        text-align: left;
    }
    
    header#native-header > nav  ul > li
    {
        opacity:1;
        position: relative;
    }

    #native-header #small-pagpage-name {
        display: block;
    }

    #native-header li:has(#large-pagpage-name) {
        display: none;
    }

    #native-header #toggle-nav {
        display: inline-block;
    }


    #native-header nav > ul {
        flex-direction: column;
    }

    header#native-header > nav > ul {
        display: none;
    }

    header#native-header[data-visible] nav > ul {
       display: flex;
    }

    header#native-header nav > ul > li
    {
        border-left: none !important;
        border-right: none !important;
    }

    body header#native-header > nav ul > li > ul
    {
        display: none;
        position: static;
    }
    
    header#native-header  li > ul[expanded]
    {
        display: block;
    }

    #native-header nav > ul > li button + p,
    #native-header nav > ul > li button + a
    {
        max-width:90%;
    }

    /* #endregion Header */

    img
    {
        display: block;
        margin: auto;
    }
}